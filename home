                    for (let o in i) a[o] = i[o]
            }
            var Tt = /^[\w-:]+$/;
            async function q(e, t, n = !1, r) {
                let {
                    ownerDocument: s,
                    ownerWindow: i,
                    fontFamilies: a
                } = t;
                if (s && Ue(e)) return r && /\S/.test(e.data) && r(e.data), s.createTextNode(e.data);
