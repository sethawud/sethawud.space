            }

            function be(e) {
                return /url\((['"]?)([^'"]+?)\1\)/.test(e)
            }
            var ye = /url\((['"]?)([^'"]+?)\1\)/g;

            function Nt(e, t) {
                let n = [];
                return e.replace(ye, (r, s, i) => (n.push([i, ue(i, t)]), r)), n.filter(([r]) => !G(r))
            }

            function xt(e) {
                let t = e.replace(/([.*+?^${}()|\[\]\/\\])/g, "\\$1");
                return new RegExp(`(url\\(['"]?)(${t})(['"]?\\))`, "g")
            }
            var It = ["background-image", "border-image-source", "-webkit-border-image", "-webkit-mask-image", "list-style-image"];

