        <div data-replit-metadata="client/src/components/Awards.tsx:55:6" data-component-name="div" class="container mx-auto px-4 sm:px-6 lg:px-8">
            <div data-replit-metadata="client/src/components/Awards.tsx:56:8" data-component-name="motion.div" class="text-center mb-16" style="opacity: 1; transform: none;">
                <h2 data-replit-metadata="client/src/components/Awards.tsx:62:10" data-component-name="h2" class="text-3xl sm:text-4xl font-bold font-poppins relative inline-block text-gray-900 dark:text-white">Honors &amp; Awards<span data-replit-metadata="client/src/components/Awards.tsx:64:12" data-component-name="span" class="absolute -bottom-2 left-0 w-full h-1 bg-primary rounded-full"></span></h2>
