                    o.setProperty(f, u, h)
                }), l
            }

            function Et(e, t) {
                (We(e) || Ge(e) || Ve(e)) && t.setAttribute("value", e.value)
            }
            var bt = [":before", ":after"],
                yt = [":-webkit-scrollbar", ":-webkit-scrollbar-button", ":-webkit-scrollbar-thumb", ":-webkit-scrollbar-track", ":-webkit-scrollbar-track-piece", ":-webkit-scrollbar-corner", ":-webkit-resizer"];

            function vt(e, t, n, r, s) {
