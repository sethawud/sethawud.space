                        if (a === r) return `src: ${i};`
                    }
                }) : e
            }
            async function kt(e, t) {
                let n = await V(e, t);
                if (v(n.node) && _(n.node)) return n.node;
                let {
                    ownerDocument: r,
                    log: s,
